{
  "name": "s3-doc-mcp",
  "version": "0.0.0",
  "description": "HTTP MCP server for S3 documentation with RAG using HNSWLib and Ollama",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts --max-warnings 0",
    "check": "npm run type-check && npm run lint:check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "s3",
    "rag",
    "ollama",
    "hnswlib",
    "documentation"
  ],
  "author": "Yoan Bernabeu",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@langchain/community": "^0.3.18",
    "@langchain/core": "^0.3.24",
    "@langchain/textsplitters": "^0.1.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "hnswlib-node": "^3.0.0",
    "ollama": "^0.5.11",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.901.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
